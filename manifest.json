{
  "manifest_version": 3,
  "name": "Carcassonne Tile Fixer",
  "description": "Fix broken tile images on BGA",
  "version": "1.0",
  "action": {
    "default_icon": "tile.png"
  },
  "icons": {
    "128": "tile.png"
  },
  "content_scripts": [
    {
      "matches": ["https://boardgamearena.com/archive/replay/*"],
      "js": ["fix.js"]
    }
  ]
}